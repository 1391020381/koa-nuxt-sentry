{
  "name": "ishare-payment-system",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "stop": "pm2 stop pm2.json",
    "local": "cross-env NODE_ENV=local nodemon ./server/index.js",
    "dev-build": "cross-env NODE_ENV=dev nuxt build",
    "dev-start": "cross-env NODE_ENV=dev  pm2 start pm2.json",
    "test-build": "cross-env NODE_ENV=test nuxt build",
    "test-start": "cross-env NODE_ENV=test  pm2 start pm2.json",
    "pre-build": "cross-env NODE_ENV=pre nuxt build",
    "pre-start": "cross-env NODE_ENV=pre  pm2 start pm2.json",
    "prod-build": "cross-env NODE_ENV=prod nuxt build",
    "prod-start": "cross-env NODE_ENV=prod  pm2 start pm2.json",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/sentry": "^5.0.0",
    "@nuxtjs/toast": "^3.3.1",
    "core-js": "^3.8.2",
    "koa": "^2.13.1",
    "nuxt": "^2.14.12",
    "pm2": "^4.5.1"
  },
  "devDependencies": {
    "@sentry/cli": "^1.61.0",
    "@sentry/webpack-plugin": "^1.14.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.4.1",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "nodemon": "^2.0.7",
    "vconsole": "^3.4.0"
  }
}